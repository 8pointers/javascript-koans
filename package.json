    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "jquery": "3.6.0",
    "jquery-ui-dist": "1.12.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "__": false
    },
    "rules": {
      "consistent-this": [
        "error",
        "fn",
        "self"
      ]
    }
  },
  "jest": {
    "globals": {
      "__": "Fill this in so that the test is passing"
    },
    "transform": {
      "1-intro|2-objects|6-functions|7-oop|8-observable|11-redux": "babel-jest"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true
  }
